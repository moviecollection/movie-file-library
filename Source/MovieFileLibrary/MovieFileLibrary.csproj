<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard1.0;netstandard2.0;net451</TargetFrameworks>
    <Description>Movie File Library is a simple C# library that helps you to get basic movie/series info from a file.</Description>
    <Authors>Peyman Mohammadi</Authors>
    <Copyright>© 2018 Peyman Mohammadi</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl></PackageProjectUrl>
    <RepositoryUrl>https://github.com/peymanr34/moviefilelibrary</RepositoryUrl>
    <PackageReleaseNotes>Change Name property to Title to avoid confusion.</PackageReleaseNotes>
    <PackageTags>movie</PackageTags>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <Version>1.0.2</Version>
    <AssemblyVersion>1.0.0.0</AssemblyVersion>
    <FileVersion>1.0.2.0</FileVersion>
  </PropertyGroup>

</Project>
